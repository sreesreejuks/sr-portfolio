{
  "cleanUrls": true,
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/"
      }
  ],
  "trailingSlash": false,
   "headers":[
       {
            "source": "/(.*)",
             "headers" : [
                 {
                    "key": "Cache-Control",
                     "value": "max-age=0"
                 }
              ]
       }
    ]
}